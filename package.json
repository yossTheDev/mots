{
	"name": "mots",
	"version": "0.0.4",
	"description": "My Own Tiny Serve. Simple and usefull local server to host your static files",
	"author": "Yoannis Sanchez Soto",
	"bin": "./bin/run",
	"homepage": "https://github.com/yossTheDev/mots",
	"license": "MIT",
	"main": "dist/index.js",
	"repository": "yossTheDev/mots",
	"files": [
		"/bin",
		"/dist",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"dependencies": {
		"@oclif/core": "^1.13.10",
		"@oclif/plugin-help": "^5",
		"@oclif/plugin-plugins": "^2.0.1",
		"cors": "^2.8.5",
		"express": "^4.18.1"
	},
	"devDependencies": {
		"@oclif/test": "^2",
		"@types/chai": "^4",
		"@types/express": "^4.17.13",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.9.4",
		"chai": "^4",
		"eslint": "^7.32.0",
		"eslint-config-oclif": "^4",
		"eslint-config-oclif-typescript": "^1.0.2",
		"eslint-config-prettier": "^8.5.0",
		"globby": "^11",
		"mocha": "^9",
		"oclif": "^3",
		"prettier": "2.7.1",
		"shx": "^0.3.3",
		"ts-node": "^10.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"oclif": {
		"bin": "mots",
		"dirname": "mots",
		"commands": "./dist/commands",
		"plugins": [
			"@oclif/plugin-help"
		],
		"topicSeparator": " "
	},
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts --config .eslintrc",
		"postpack": "shx rm -f oclif.manifest.json",
		"posttest": "yarn lint",
		"prepack": "yarn build && oclif manifest && oclif readme",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif readme && git add README.md"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"bugs": "https://github.com/Apps/hello-world/issues",
	"keywords": [
		"oclif"
	],
	"types": "dist/index.d.ts"
}
